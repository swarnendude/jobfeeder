# Theirstack API Key - Get from https://theirstack.com
THEIRSTACK_API_KEY=your_theirstack_api_key_here

# Anthropic Claude API Key - Get from https://console.anthropic.com
# Used for advanced AI analysis (job analysis, cover letters, job matching)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini API Key - Get from https://aistudio.google.com/apikey
# Used for company data structuring (more cost-effective for basic extraction)
# If set, Gemini will be used for company enrichment instead of Claude
GEMINI_API_KEY=your_gemini_api_key_here

# SignalHire API Key - Get from https://www.signalhire.com/api
# Used for enriching contact information (email, phone) of target contacts
SIGNALHIRE_API_KEY=your_signalhire_api_key_here

# Server Port (optional, defaults to 3000)
PORT=3000
